# This file is a "template" of which configurations need to be defined for your application
# Copy this file to this folder as "application.properties" for development

# Web
webserver.port=443
ssl.certificate.key.file=path/
ssl.certificate.chain.file=path/

# Postgres
postgres.database=name_of_db
postgres.host=host_of_db
postgres.port=1234
postgres.user=postgres_user
postgres.password=postgres_password

# Fitbit
fitbit.api.port=443
fitbit.oauth2.redirect.uri=http://localhost:8180/login
fitbit.oauth2.return.uri=http://localhost:8180/success
fitbit.oauth2.scope=activity heartrate profile sleep weight
fitbit.oauth2.subscription.uri=1/user/-/apiSubscriptions/[subscription-id].json
fitbit.client.id=123
fitbit.client.secret=afefw
fitbit.verify.code=fljdslkfas

# Fitbit scheduler
fitbit.pull.enabled=false
fitbit.pull.interval.minutes=15

# Garmin
garmin.consumer.key=123
garmin.consumer.secret=123
garmin.callback.url=http://localhost:8082/callback

# Kafka producer properties
kafka.string.serializer=org.apache.kafka.common.serialization.StringSerializer
kafka.omh.serializer=dtu.openhealth.integration.shared.util.serialization.OmhDTOSerializer
kafka.acks=1

# Kafka consumer properties
kafka.string.deserializer=org.apache.kafka.common.serialization.StringDeserializer
kafka.omh.deserializer="dtu.openhealth.integration.shared.util.serialization.OmhDTODeserializer
kafka.group.id=my_group
kafka.auto.offset.reset=earliest
kafka_enable_auto_commit=false

# Kafka general properties
kafka.bootstrap.servers=localhost:9092
kafka.topic=omh-data



